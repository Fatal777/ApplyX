{
  "annotations": {
    "list": []
  },
  "description": "ApplyX Application Dashboard - Monitor API performance, database, cache, AI services, and interview metrics",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          },
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "id": 2,
      "title": "Request Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(applyx_http_requests_total[5m]))",
          "legendFormat": "Requests/sec"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 0.5 }
            ]
          },
          "unit": "s"
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "id": 3,
      "title": "Avg Latency (p50)",
      "type": "stat",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(applyx_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "id": 4,
      "title": "Error Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(applyx_http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(applyx_http_requests_total[5m])) * 100",
          "legendFormat": "Error %"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "id": 5,
      "title": "Active Connections",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(applyx_http_requests_in_progress)",
          "legendFormat": "In Progress"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 10,
      "title": "HTTP Metrics",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 1, "fillOpacity": 20 },
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 11,
      "title": "Request Rate by Endpoint",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(applyx_http_requests_total[5m])) by (endpoint)",
          "legendFormat": "{{endpoint}}"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 1, "fillOpacity": 20 },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 12,
      "title": "Latency Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(applyx_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(applyx_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(applyx_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 20,
      "title": "Database & Cache",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 1, "fillOpacity": 20 },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 15 },
      "id": 21,
      "title": "Database Query Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(applyx_db_query_duration_seconds_bucket[5m])) by (le, operation))",
          "legendFormat": "{{operation}} p95"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 1, "fillOpacity": 20 },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 15 },
      "id": 22,
      "title": "Redis Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(applyx_redis_operation_duration_seconds_bucket[5m])) by (le, operation))",
          "legendFormat": "{{operation}} p95"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "stacking": { "mode": "percent" } },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 15 },
      "id": 23,
      "title": "Cache Hit Ratio",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(applyx_cache_operations_total{result=\"hit\"}[5m]))",
          "legendFormat": "Hits"
        },
        {
          "expr": "sum(rate(applyx_cache_operations_total{result=\"miss\"}[5m]))",
          "legendFormat": "Misses"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "id": 30,
      "title": "AI Services",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 1, "fillOpacity": 20 },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "id": 31,
      "title": "AI Request Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(applyx_ai_request_duration_seconds_bucket[5m])) by (le, provider, operation))",
          "legendFormat": "{{provider}} {{operation}} p95"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 24 },
      "id": 32,
      "title": "AI Token Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(applyx_ai_tokens_total[1h])) by (provider, type)",
          "legendFormat": "{{provider}} {{type}}"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 24 },
      "id": 33,
      "title": "AI Request Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(applyx_ai_requests_total[5m])) by (provider, status)",
          "legendFormat": "{{provider}} {{status}}"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "id": 40,
      "title": "Interview Platform",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 33 },
      "id": 41,
      "title": "Interview Sessions",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(applyx_interview_sessions_total[1h])) by (type, status)",
          "legendFormat": "{{type}} - {{status}}"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 33 },
      "id": 42,
      "title": "Interview Duration",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(applyx_interview_duration_seconds_bucket[1h])) by (le, type))",
          "legendFormat": "{{type}} median"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 70 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 33 },
      "id": 43,
      "title": "Interview Score Distribution",
      "type": "histogram",
      "targets": [
        {
          "expr": "sum(applyx_interview_score_bucket) by (le)",
          "legendFormat": "{{le}}"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 },
      "id": 50,
      "title": "Celery Tasks",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 42 },
      "id": 51,
      "title": "Task Throughput",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(applyx_celery_tasks_total[5m])) by (task_name, status)",
          "legendFormat": "{{task_name}} - {{status}}"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 42 },
      "id": 52,
      "title": "Task Duration",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(applyx_celery_task_duration_seconds_bucket[5m])) by (le, task_name))",
          "legendFormat": "{{task_name}} p95"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["applyx", "backend", "api"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "label": "Data Source",
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "ApplyX Application Dashboard",
  "uid": "applyx-main",
  "version": 1
}
